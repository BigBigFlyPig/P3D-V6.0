        <PropertyDef
            id          = "{1428FA44-DD04-4962-8262-A3136F658D90}"
            name        = "ObjectRayLengthMax"
            type        = "FLOAT"
            serialize   = "default"
            default     = "0"
            descr       = "The maximum length of the ray trace when designating objects in meters." />
            
        <PropertyDef
            id          = "{755414C0-22C4-4C54-8BA9-567008493166}"
            name        = "ObjectMinimumGranularity"
            type        = "FLOAT"
            serialize   = "default"
