        <PropertyDef
            id          = "{00EAE7D5-8DE1-4F6F-9355-766C9DE6BA07}"
            name        = "TerrainRayLengthMax"
            type        = "FLOAT"
            serialize   = "default"
            default     = "0"
            descr       = "The maximum length of the ray trace when designating terrain in meters." />
            
        <PropertyDef
            id          = "{8D2CC07C-3AA2-4BA6-AE62-BD516D21A365}"
            name        = "TerrainMinimumGranularity"
            type        = "FLOAT"
